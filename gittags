git tag -l --- lists all the tags in the branch 
git tag -d 'tagname' -- to delete a tag 
git push origin --tags  --- to push all the tags to origin after creating locally 

#Delete all local tags.
git tag -d $(git tag -l)
#Fetch remote tags.
git fetch
#Delete remote tags.
git push origin --delete $(git tag -l) # Pushing once should be faster than multiple times
#Delete local tags.
git tag -d $(git tag -l)
